{
  "name": "nest-angular-starter",
  "version": "0.0.0",
  "scripts": {
    "test:client": "ng test",
    "e2e:client": "ng e2e",
    "lint:client": "ng lint",
    "test:server": "jest --config=jest.server.config.json",
    "e2e:server": "jest --config ./server-e2e/jest-e2e.json --detectOpenHandles",
    "build:client-browser": "ng build --prod",
    "build:client-server": "ng run nest-angular-starter:server:production",
    "build:server": "tsc -p ./server/tsconfig.build.json",
    "build": "npm run del && npm run build:client-browser && npm run build:client-server && npm run build:server",
    "del": "rimraf dist",
    "prestart": "npm run build",
    "start": "NODE_ENV=production node -r ./tsconfig-paths-bootstrap.js dist/server/main.js",
    "server": "nodemon",
    "server:debug": "nodemon --config nodemon-debug.json",
    "client": "ng serve",
    "format": "prettier --write ./server/**/*.ts && prettier --write ./client/**/*.ts"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.2.0",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/forms": "~7.2.0",
    "@angular/http": "~7.2.0",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/platform-server": "~7.2.0",
    "@angular/router": "~7.2.0",
    "@nestjs/common": "^6.0.4",
    "@nestjs/core": "^6.0.4",
    "@nestjs/ng-universal": "^0.4.2",
    "@nestjs/passport": "^6.0.0",
    "@nestjs/platform-express": "^6.0.4",
    "@nestjs/swagger": "^3.0.2",
    "@nestjs/typeorm": "^6.0.0",
    "@nguniversal/express-engine": "^7.1.1",
    "@nguniversal/module-map-ngfactory-loader": "^7.1.1",
    "automapper-ts-node": "^1.0.2",
    "cache-manager": "^2.9.0",
    "class-transformer": "^0.2.0",
    "class-validator": "^0.9.1",
    "compression": "^1.7.4",
    "config": "^3.0.1",
    "core-js": "^2.5.4",
    "express-rate-limit": "^3.4.0",
    "fastify-formbody": "^3.1.0",
    "helmet": "^3.16.0",
    "mysql": "^2.16.0",
    "passport": "^0.4.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^2.6.3",
    "rxjs": "^6.4.0",
    "swagger-ui-express": "^4.0.2",
    "tslib": "^1.9.0",
    "typeorm": "^0.2.16",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular/cli": "~7.3.5",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@nestjs/testing": "^6.0.4",
    "@types/config": "0.0.34",
    "@types/express": "^4.16.1",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/jest": "^24.0.11",
    "@types/node": "^8.10.45",
    "@types/supertest": "^2.0.7",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "jest": "^24.5.0",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "nodemon": "^1.18.10",
    "prettier": "^1.16.4",
    "protractor": "~5.4.0",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.0",
    "ts-loader": "^5.3.3",
    "ts-node": "^8.0.3",
    "tsconfig-paths": "^3.8.0",
    "tslint": "^5.11.0",
    "typescript": "~3.2.2"
  }
}
